{
  "expo": {
    "name": "jobs",
    "slug": "jobs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "Jobs",
      "config": { "googleMaps": { "apiKey": "이 곳에 Api키를 넣어주세요" } }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
